<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_graph"
    tools:ignore="InvalidNavigation">
    <fragment
        android:id="@+id/avatar"
        android:name="com.example.fragment.module.user.fragment.UserAvatarFragment"
        android:label="UserAvatarFragment"
        tools:layout="@layout/user_avatar_fragment">
        <deepLink app:uri="fragment.example.com/user/avatar" />
    </fragment>
    <fragment
        android:id="@+id/city"
        android:name="com.example.fragment.module.user.fragment.UserCityFragment"
        android:label="UserCityFragment"
        tools:layout="@layout/user_city_fragment">
        <deepLink app:uri="fragment.example.com/user/city" />
    </fragment>
    <fragment
        android:id="@+id/info"
        android:name="com.example.fragment.module.user.fragment.UserInfoFragment"
        android:label="UserInfoFragment"
        tools:layout="@layout/user_info_fragment">
        <deepLink app:uri="fragment.example.com/user/info" />
    </fragment>
    <fragment
        android:id="@+id/login"
        android:name="com.example.fragment.module.user.fragment.UserLoginFragment"
        android:label="UserLoginFragment">
        <deepLink app:uri="fragment.example.com/user/login" />
    </fragment>
    <fragment
        android:id="@+id/register"
        android:name="com.example.fragment.module.user.fragment.UserRegisterFragment"
        android:label="UserRegisterFragment">
        <deepLink app:uri="fragment.example.com/user/register" />
    </fragment>
    <fragment
        android:id="@+id/user_share"
        android:name="com.example.fragment.module.user.fragment.UserShareFragment"
        android:label="UserShareFragment"
        tools:layout="@layout/user_share_fragment">
        <deepLink app:uri="fragment.example.com/user/share" />
    </fragment>
</navigation>